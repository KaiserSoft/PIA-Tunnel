client
dev tun
proto tcp
remote us-east.privateinternetaccess.com 501
resolv-retry infinite
nobind
persist-key
persist-tun
cipher aes-256-cbc
auth sha256
tls-client
remote-cert-tls server
auth-user-pass /usr/local/pia/login-pia.conf 
comp-lzo
verb 1
reneg-sec 0
crl-verify /pia/ovpn/PIAtcpHIGH/crl.rsa.4096.pem
ca /pia/ovpn/PIAtcpHIGH/ca.rsa.4096.crt

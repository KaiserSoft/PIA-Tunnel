# force proper line endings for certain files
*.odt -text
*.pdf -text
*.png -text
*.gif -text
*.jpg -text
*.vbs text eol=crlf
*.pattern text eol=crlf
*.ini text eol=crlf
*.sh text eol=lf
pia-* text eol=lf
reset-pia text eol=lf
*.conf text eol=lf
*.service text eol=lf
*.local text eol=lf
devfs.rules text eol=lf